<table id="listaCortar" class="table table-striped" style="width:100%">
  <thead>
    <tr>
      <th>NÂ° Carrete</th>
      <th>Usuario</th>
      <th>Fecha corte</th>
      <th>Metraje inicial</th>
      <th>Metraje cortado</th>
      <th>Metraje final</th>
      <th>Unidad de medida</th>
    </tr>
  </thead>
  <tbody>
    {% for cortado in cortados %}
      <tr>
        <td>{{ cortado.carrete }}</td>
        <td>{{ cortado.usuario }}</td>
        <td>{{ cortado.fecha }}</td>
        <td>{{ cortado.inicial }}</td>
        <td>{{ cortado.corte }}</td>
        <td>{{ cortado.final }}</td>
        <td>{{ cortado.unidad }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
